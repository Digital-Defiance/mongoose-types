{
  "name": "@digitaldefiance/mongoose-types",
  "version": "8.20.8",
  "homepage": "https://github.com/Digital-Defiance/mongoose-types#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Digital-Defiance/mongoose-types.git"
  },
  "description": "Mongoose TypeScript definitions with support for custom ID types (string | ObjectId | any)",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "yarn test:types && yarn test:tsd && yarn test:runtime",
    "test:types": "tsc --noEmit --skipLibCheck test-types.ts",
    "test:tsd": "tsd",
    "test:runtime": "node test-runtime.js",
    "prepublishOnly": "yarn test",
    "publish:public": "npm publish --access public"
  },
  "dependencies": {
    "mongoose": "^8.20.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsd": "^0.33.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "mongoose": "^8.20.0"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "types",
    "typescript",
    "custom-id",
    "string-id",
    "uuid",
    "guid"
  ],
  "author": "Digital Defiance, Jessica Mulein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Digital-Defiance/mongoose-types/issues"
  }
}
